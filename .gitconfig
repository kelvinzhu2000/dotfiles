[alias]
    lol = log --pretty=oneline --abbrev-commit --graph --decorate

[color]
    ui = auto

[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green

[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold

[color "status"]
    added = yellow
    changed = green
    untracked = cyan

[apply]
    whitespace = nowarn

[push]
    default = current

[core]
    autocrlf = input
    editor = vim
